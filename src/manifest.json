{
  "name": "Balloonmark",
  "options_page": "options.html",
  "background": {
    "scripts": ["background.bundle.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon-34.png",
    "default_title": "balloonmark"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": ["bookmarks", "unlimitedStorage", "storage", "tabs"]
}
